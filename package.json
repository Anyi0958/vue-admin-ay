{
  "name": "ay-vue-admin",
  "version": "0.1.0",
  "author": "AY<anyi0958@163.com>",
  "private": false,
  "scripts": {
    "init": "npm install || npm install -g --registry=https://registry.npm.taobao.org",
    "dev": "vue-cli-service serve",
    "inspect": "vue-cli-service inspect",
    "build": "vue-cli-service build",
    "build:report": "vue-cli-service build --report",
    "plop": "plop",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint --fix",
    "eslint": "eslint --ext .js,.vue src --fix",
    "lint:style": "stylelint-config-prettier-check",
    "clear": "rimraf node_modules && rimraf dist",
    "update": "ncu -u&&npm i",
    "update:globle": "ncu -g --concurrency 10 --timeout 80000",
    "push": "start ./push.sh",
    "deploy": "start ./deploy.sh",
    "preview": "node build/index.js --preview"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.1",
    "element-ui": "^2.13.2",
    "js-cookie": "^2.2.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "view-design": "^4.3.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.22.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "compression-webpack-plugin": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "plop": "^2.7.4",
    "prettier": "^2.1.1",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "script-ext-html-webpack-plugin": "2.1.3",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.1.0",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "main": "babel.config.js",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "vue",
    "admin",
    "admin-template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anyi0958/vue-admin-ay.git"
  }
}
